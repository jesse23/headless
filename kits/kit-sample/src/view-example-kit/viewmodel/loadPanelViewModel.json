{    
    "schemaVersion": "1.0.0",
    "name": "LoadPanel",
    "lifecycleHooks": {
        "onMount": "loadData"
    },
    "data": {
        "dataprovider": {
            "simpleListExample": {
                "loadedViewModelObjects": []
            }
        }
    },
  "actions": {
    "loadData": {
      "actionType": "JSFunction",
      "method": "loadData",
      "inputData": {},
      "outputData": {
        "dataprovider.simpleListExample.loadedViewModelObjects": "data",
        "dataprovider.simpleListExample.totalFound": "totalFound"
      },
      "deps": "js/loadPanelService"
    }
  },
  "imports": [
    "SimpleList"
  ]
}