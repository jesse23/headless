{
  "name": "@headless/transform",
  "version": "1.0.0",
  "description": "Transform view and view model to js code",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "export": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./register": {
      "import": "./dist/register.js",
      "require": "./dist/register.js"
    }
  },
  "scripts": {
    "audit": "eslint src test && tsc",
    "build": "tsx esbuild",
    "test": "vitest run",
    "clean": "rimraf dist doc *.log *.tgz node_modules/.*"
  },
  "devDependencies": {
    "@headless/utils": "^1.0.0",
    "@headless/types": "^1.0.0",
    "@headless/esbuild": "^1.0.0",
    "@headless/vitest": "^1.0.0"
  },
  "author": "jesse23",
  "license": "ISC"
}
