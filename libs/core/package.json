{
  "name": "@headless/core",
  "version": "1.0.0",
  "description": "Abstracted view model layer",
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "export": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./register": {
      "import": "./dist/register.js",
      "require": "./dist/register.js"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.js"
    }
  },
  "dependencies": {
    "@headless/interop": "^1.0.0",
    "@headless/transform": "^1.0.0"
  },
  "devDependencies": {
    "@headless/types": "^1.0.0",
    "@headless/utils": "^1.0.0",
    "@headless/esbuild": "^1.0.0",
    "@headless/vitest": "^1.0.0"
  },
  "scripts": {
    "audit": "eslint src test && tsc",
    "build": "npm run build:src",
    "build:src": "tsx esbuild",
    "test": "vitest run",
    "clean": "rimraf dist doc *.log *.tgz node_modules"
  },
  "author": "jesse23",
  "license": "ISC"
}
