{
  "name": "stencil-sample",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-sample/stencil-sample.esm.js",
  "exports": {
    ".": {
      "import": "./dist/stencil-sample/stencil-sample.esm.js",
      "require": "./dist/stencil-sample/stencil-sample.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "audit": "echo \"placeholder\" && exit 0",
    "test": "echo \"placeholder\" && exit 0",
    "start": "stencil build --dev --watch --serve --port 5173",
    "build": "stencil build",
    "clean": "rimraf dist doc *.log *.tgz node_modules .stencil loader",
    "test.ootb": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "npm": "^10.9.0",
    "puppeteer": "^21.9.0",
    "unplugin-auto-import": "^0.18.3",
    "@headless/core": "^1.0.0",
    "@headless/reactivity": "^1.0.0",
    "@headless/components": "^1.0.0"
  },
  "license": "MIT"
}
